<!DOCTYPE html>
<html>
<head>
  <title>OWTBoatsManager</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="container">
  <% if user_signed_in? %>
    <%= link_to('Signout', destroy_user_session_path, :method => :delete, class: 'btn btn-default') %>
  <% end %>

  <%= yield %>
</div>

</body>
</html>
